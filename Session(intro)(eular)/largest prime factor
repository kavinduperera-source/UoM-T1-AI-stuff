def largest_prime_factor(number):
    largest_factor = -1
    
    while number % 2 == 0:
        largest_factor = 2
        number //= 2

    i = 3
    while i * i <= number:
        while number % i == 0:
            largest_factor = i
            number //= i
        i += 2

    if number > 1:
        largest_factor = number

    return largest_factor

print(largest_prime_factor(600851475143))

